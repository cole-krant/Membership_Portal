                      <div class="card mb-3">
                          <table class="table">
                              <thead>
                                  <br>
                                  <h3 style="color:black">Basic Information</h3>
                                  <p>Editing User: <%= profile[0].name  %></p>
                                  <br>
                              </thead>
                              
                              <tbody>
                                  <tr>
                                      <form action="/update_profile/name-admin" method="POST">
                                          <td style="width:15%"><h5 style="color:black; text-align: center;"> Name </h5></td>
                                          <td style="max-width:100%">
                                              <input class="w3-input w3-border" type="text" name="name" placeholder="Enter your preferred name or nickname" required></input>
                                          </td>
                                          <td style="max-width:5%"><button action="submit" class="btn default" style="background-color: rgb(4, 125, 153); color: white">Save Name</button></td>
                                      </form>
                                  </tr>
                                  <tr>
                                      <form action="/update_profile/class-admin" method="POST">
                                          
                                          <td><h5 style="color:black; text-align: center;"> Class </h5></td>
                                          <td>
                                              <select class="form-select" id="class" name="class">
                                                  <option selected>Choose...</option>
                                                  <option value="Freshman">Freshman</option>
                                                  <option value="Sophomore">Sophomore</option>
                                                  <option value="Junior">Junior</option>
                                                  <option value="Senior">Senior</option>
                                              </select>
                                          </td>
                                          <td><button action="submit" class="btn default" style="background-color: rgb(4, 125, 153); color: white">Save Class</button></td>
                                      </form>
                                  </tr>
                                  <tr>
                                      <form action="/update_profile/major-admin" method="POST">
